package defaultmasquerades

import "github.com/getlantern/fronted"

var TrustedCAs = []*CA{ {{range .cas}}
	&CA{
		CommonName: "{{.CommonName}}",
		Cert:       "{{.Cert}}",
	}, {{end}}
}

var Cloudflare = []*fronted.Masquerade{}

var Cloudfront = []*fronted.Masquerade{ {{range .masquerades}}
	&fronted.Masquerade{
		Domain:    "{{.Domain}}",
		IpAddress: "{{.IpAddress}}",
	}, {{end}}
}
